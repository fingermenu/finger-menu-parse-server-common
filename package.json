{
  "name": "@fingermenu/parse-server-common",
  "version": "0.0.48",
  "description": "Finger Menu Parse Server Common",
  "main": "./dist/index.js",
  "scripts": {
    "start": "babel-node src/index.js",
    "build": "babel src --out-dir dist",
    "lint": "eslint --ext .jsx --ext .js src",
    "fixlint": "eslint --fix --ext .jsx --ext .js src",
    "test": "jest",
    "test:watch": "jest --watch",
    "publish-package": "npm run build && npm publish",
    "flow": "flow",
    "precommit": "lint-staged"
  },
  "author": "FingerMenu ltd<fingermenu.ltd@gmail.com>",
  "homepage": "https://bitbucket.org/fingermenu/finger-menu-parse-server-common#readme",
  "dependencies": {
    "@microbusiness/common-javascript": "^1.1.13",
    "@microbusiness/parse-server-common": "^1.1.33",
    "immutable": "^4.0.0-rc.9"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "chance": "^1.0.13",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.46.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.12.2",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.66.0",
    "husky": "^0.14.3",
    "jest": "^22.4.0",
    "lint-staged": "^7.0.0",
    "parse": "^1.11.0",
    "prettier": "^1.10.2",
    "uuid": "^3.2.1"
  },
  "jest": {
    "rootDir": "./src"
  },
  "pre-commit": "lint:staged",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org:fingermenu/finger-menu-parse-server-common.git"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --write --single-quote true --trailing-comma all --print-width 150",
      "npm run fixlint",
      "git add"
    ]
  }
}
